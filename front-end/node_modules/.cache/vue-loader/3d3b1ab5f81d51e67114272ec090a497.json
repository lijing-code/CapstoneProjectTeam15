le":"/Users/jingli/Desktop/tecmi/frontend/tecmi-ecommerce-frontend/tecmi-ecommerce-frontend/src/views/Product/Wishlist.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-12 text-center\">\n        <h4 class=\"pt-3\">Your WishList</h4>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div v-for=\"product of products\" :key=\"product.id\" \n      class=\"col-md-6 col-xl-4 col-12 pt-3  justify-content-around d-flex\">\n        <ProductBox :product=\"product\">\n        </ProductBox>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProductBox from '../../components/Product/ProductBox';\nexport default {\n  data() {\n      return {\n          products:null,\n          token:null\n      }\n  },\n  name: 'Product',\n  components : {ProductBox},\n  props : [ \"baseURL\"],\n  methods : {\n    fetchWishlist() {\n\n      // fetch products\n      axios.get(`${this.baseURL}wishlist/${this.token}`)\n        .then(data => this.products = data.data)\n        .catch(err => console.log(err));\n    }\n  },\n  mounted() {\n        this.token = localStorage.getItem('token');\n        this.fetchWishlist();\n    }\n}\n</script>\n\n<style scoped>\nh4 {\n  font-family: 'Roboto', sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n</style>\n"]}]}