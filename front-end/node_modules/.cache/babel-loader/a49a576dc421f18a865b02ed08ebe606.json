AI,CAAC,SAAD,EAAY,UAAZ,EAAwB,YAAxB,CANK;AAOb,EAAA,OAAM,EAAI;AACR,UAAM,WAAN,GAAoB;AAClB,MAAA,KAAK,CAAC,IAAN,CAAW,KAAK,OAAL,GAAa,iBAAb,GAA+B,KAAK,EAA/C,EAAmD,KAAK,OAAxD,EACC,IADD,CACM,GAAE,IAAK;AACX;AACA,aAAK,KAAL,CAAW,WAAX;AACA,aAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,UAAA,IAAG,EAAI;AAAR,SAAlB;AACA,QAAA,IAAI,CAAC;AACH,UAAA,IAAI,EAAE,+BADH;AAEH,UAAA,IAAI,EAAE,SAFH;AAGH,UAAA,mBAAmB,EAAE;AAHlB,SAAD,CAAJ;AAKD,OAVD,EAWC,KAXD,CAWO,GAAE,IAAK,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAmB,GAAnB,CAXd;AAYF;;AAdQ,GAPG;;AAuBb,EAAA,OAAO,GAAG;AACR,QAAI,CAAC,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAL,EAAoC;AAClC,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,QAAA,IAAG,EAAI;AAAR,OAAlB;AACA;AACF;;AACA,SAAK,EAAL,GAAU,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA7B;AACA,SAAK,OAAL,GAAe,KAAK,QAAL,CAAc,IAAd,CAAmB,OAAM,IAAK,OAAO,CAAC,EAAR,IAAc,KAAK,EAAjD,CAAf;AACF;;AA9Ba,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-12 text-center\">\n        <h4 class=\"pt-3\">Edit Product</h4>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-3\"></div>\n      <div class=\"col-md-6 px-5 px-md-0\">\n        <form v-if=\"product\">\n          <div class=\"form-group\">\n            <label>Category</label>\n            <select class=\"form-control\" v-model=\"product.categoryId\" required>\n              <option v-for=\"category of categories\" :key=\"category.id\" :value=\"category.id\">{{category.categoryName}}</option>\n            </select>\n          </div>\n          <div class=\"form-group\">\n            <label>Name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"product.name\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>Description</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"product.description\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>ImageURL</label>\n            <input type=\"url\" class=\"form-control\" v-model=\"product.imageURL\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>Price</label>\n            <input type=\"number\" class=\"form-control\" v-model=\"product.price\" required>\n          </div>\n          <button type=\"button\" class=\"btn btn-primary\" @click=\"editProduct\">Submit</button>\n        </form>\n      </div>\n      <div class=\"col-3\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data(){\n    return {\n      product: null\n    }\n  },\n  props : [\"baseURL\", \"products\", \"categories\"],\n  methods : {\n    async editProduct() {\n      axios.post(this.baseURL+\"product/update/\"+this.id, this.product)\n      .then(res => {\n        //sending the event to parent to handle\n        this.$emit(\"fetchData\");\n        this.$router.push({name : 'AdminProduct'});\n        swal({\n          text: \"Product Updated Successfully!\",\n          icon: \"success\",\n          closeOnClickOutside: false,\n        });\n      })\n      .catch(err => console.log(\"err\", err));\n    }\n  },\n  mounted() {\n    if (!localStorage.getItem('token')) {\n      this.$router.push({name : 'Signin'});\n      return;\n    }\n    this.id = this.$route.params.id;\n    this.product = this.products.find(product => product.id == this.id);\n  }\n}\n</script>\n\n<style scoped>\nh4 {\n  font-family: 'Roboto', sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n\n</style>\n"],"sourceRoot":""}]}