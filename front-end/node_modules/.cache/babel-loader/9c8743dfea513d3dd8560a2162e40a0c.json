s : {\n    async fetchImages(){\n      // fetch products\n      await axios.get(this.baseURL + \"fileUpload/\")\n        .then(res => this.images = res.data)\n        .catch(err => console.log(err));\n    }\n  },\n  mounted() {\n    if (!localStorage.getItem('token')) {\n      this.$router.push({name : 'Signin'});\n      return;\n    }\n    this.fetchImages();\n  }\n}\n</script>\n\n<style scoped>\nh4 {\n  font-family: 'Roboto', sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n\n#add-image {\n  float: right;\n  font-weight: 500;\n}\n</style>\n"],"sourceRoot":""}]}