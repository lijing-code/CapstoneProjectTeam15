AAG,EAAI,IAHF;AAIL,MAAA,WAAU,EAAI,IAJT;AAKL,MAAA,QAAO,EAAI,IALN;AAML,MAAA,KAAI,EAAI;AANH,KAAP;AAQD,GAVY;;AAWb,EAAA,KAAI,EAAI,CAAC,SAAD,EAAY,UAAZ,EAAwB,YAAxB,CAXK;AAYb,EAAA,OAAM,EAAI;AACR,UAAM,UAAN,GAAmB;AACjB,YAAM,UAAS,GAAI;AACjB,QAAA,EAAC,EAAI,KAAK,EADO;AAEjB,QAAA,UAAS,EAAI,KAAK,UAFD;AAGjB,QAAA,IAAG,EAAI,KAAK,IAHK;AAIjB,QAAA,WAAU,EAAI,KAAK,WAJF;AAKjB,QAAA,QAAO,EAAI,KAAK,QALC;AAMjB,QAAA,KAAI,EAAI,KAAK;AANI,OAAnB;AASA,YAAM,KAAK,CAAC;AACV,QAAA,MAAM,EAAE,MADE;AAEV,QAAA,GAAG,EAAE,KAAK,OAAL,GAAa,aAFR;AAGV,QAAA,IAAG,EAAI,IAAI,CAAC,SAAL,CAAe,UAAf,CAHG;AAIV,QAAA,OAAO,EAAE;AACP,0BAAgB;AADT;AAJC,OAAD,CAAL,CAQL,IARK,CAQA,GAAE,IAAK;AACX;AACA,aAAK,KAAL,CAAW,WAAX;AACA,aAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,UAAA,IAAG,EAAI;AAAR,SAAlB;AACA,QAAA,IAAI,CAAC;AACH,UAAA,IAAI,EAAE,6BADH;AAEH,UAAA,IAAI,EAAE,SAFH;AAGH,UAAA,mBAAmB,EAAE;AAHlB,SAAD,CAAJ;AAKD,OAjBK,EAkBL,KAlBK,CAkBC,GAAE,IAAK,OAAO,CAAC,GAAR,CAAY,GAAZ,CAlBR,CAAN;AAmBF;;AA9BQ,GAZG;;AA4Cb,EAAA,OAAO,GAAG;AACR,QAAI,CAAC,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAL,EAAoC;AAClC,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAC,QAAA,IAAG,EAAI;AAAR,OAAlB;AACF;AACF;;AAhDa,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col-12 text-center\">\n        <h4 class=\"pt-3\">Add new Product</h4>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-3\"></div>\n      <div class=\"col-md-6 px-5 px-md-0\">\n        <form>\n          <div class=\"form-group\">\n            <label>Category</label>\n            <select class=\"form-control\" v-model=\"categoryId\" required>\n              <option v-for=\"category of categories\" :key=\"category.id\" :value=\"category.id\">{{category.categoryName}}</option>\n            </select>\n          </div>\n          <div class=\"form-group\">\n            <label>Name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"name\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>Description</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"description\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>ImageURL</label>\n            <input type=\"url\" class=\"form-control\" v-model=\"imageURL\" required>\n          </div>\n          <div class=\"form-group\">\n            <label>Price</label>\n            <input type=\"number\" class=\"form-control\" v-model=\"price\" required>\n          </div>\n          <button type=\"button\" class=\"btn btn-primary\" @click=\"addProduct\">Submit</button>\n        </form>\n      </div>\n      <div class=\"col-3\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data(){\n    return {\n      id : null,\n      categoryId : null,\n      name : null,\n      description : null,\n      imageURL : null,\n      price : null\n    }\n  },\n  props : [\"baseURL\", \"products\", \"categories\"],\n  methods : {\n    async addProduct() {\n      const newProduct = {\n        id : this.id,\n        categoryId : this.categoryId,\n        name : this.name,\n        description : this.description,\n        imageURL : this.imageURL,\n        price : this.price\n      }\n\n      await axios({\n        method: 'post',\n        url: this.baseURL+\"product/add\",\n        data : JSON.stringify(newProduct),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      })\n      .then(res => {\n        //sending the event to parent to handle\n        this.$emit(\"fetchData\");\n        this.$router.push({name : 'AdminProduct'});\n        swal({\n          text: \"Product Added Successfully!\",\n          icon: \"success\",\n          closeOnClickOutside: false,\n        });\n      })\n      .catch(err => console.log(err));\n    }\n  },\n  mounted() {\n    if (!localStorage.getItem('token')) {\n      this.$router.push({name : 'Signin'});\n    }\n  }\n}\n</script>\n\n<style scoped>\nh4 {\n  font-family: 'Roboto', sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n\n</style>\n"],"sourceRoot":""}]}